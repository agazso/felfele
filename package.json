{
  "name": "postmodern",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@types/jest": "^20.0.4",
    "@types/react": "^15.6.18",
    "@types/react-native": "^0.52.24",
    "@types/react-native-vector-icons": "^4.6.1",
    "@types/react-navigation": "^1.5.12",
    "babel-jest": "^20.0.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-node6": "^11.0.0",
    "eslint": "^4.19.1",
    "jest": "~20.0.0",
    "jest-fetch-mock": "^1.6.5",
    "mock-async-storage": "^1.0.3",
    "react-native-debugger-open": "^0.3.17",
    "react-native-typescript-transformer": "^1.2.10",
    "react-test-renderer": "16.0.0-alpha.12",
    "replace": "^1.0.0",
    "ts-jest": "^20.0.7",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2"
  },
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "node node_modules/jest/bin/jest.js --watch",
    "tsc": "tsc --skipLibCheck",
    "lint": "tslint src/**/*.ts",
    "build": "npm run clean && npm run tsc --",
    "clean": "rm -rf build",
    "postinstall": "npm run patch",
    "patch": "npm run patchAndroidInstaller && npm run deleteBabelrcFromNodeModules",
    "patchAndroidInstaller": "./node_modules/.bin/replace \\'install\\',\\ pathToApk \\'install\\',\\ \\'-r\\',\\ pathToApk node_modules/react-native/local-cli/runAndroid/runAndroid.js -s",
    "deleteBabelrcFromNodeModules": "rm -f ./node_modules/swarm-js/.babelrc && rm -f ./node_modules/eth-lib/.babelrc"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|react-navigation)/"
    ],
    "testRegex": "(/test/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/lib/",
      "<rootDir>/build/"
    ],
    "cacheDirectory": ".jest/cache"
  },
  "dependencies": {
    "buffer": "^5.0.6",
    "immutable": "^3.8.2",
    "node-libs-browser": "^2.1.0",
    "react": "16.3.1",
    "react-native": "^0.55.1",
    "react-native-crypto": "^2.1.2",
    "react-native-communications": "^2.2.1",
    "react-native-easy-grid": "^0.1.17",
    "react-native-easy-markdown": "^1.2.0",
    "react-native-gravatar": "^1.0.2",
    "react-native-image-picker": "^0.26.10",
    "react-native-parse-html": "^2.2.6",
    "react-native-qrcode": "^0.2.4",
    "react-native-settings-list": "^1.8.0",
    "react-native-util": "^1.0.2",
    "react-native-vector-icons": "^4.6.0",
    "react-native-xml2js": "^1.0.3",
    "react-navigation": "^1.5.12",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-persist": "^5.10.0",
    "redux-persist-transform-immutable": "^5.0.0",
    "redux-thunk": "^2.2.0",
    "swarm-js": "^0.1.37",
    "tslib": "^1.9.3",
    "url": "^0.11.0",
    "util": "^0.10.4"
  }
}
