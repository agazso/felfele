{
  "name": "postmodern",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@types/jest": "^23.3.8",
    "@types/react": "^16.4.18",
    "@types/react-native": "^0.57.7",
    "@types/react-native-vector-icons": "^4.6.4",
    "@types/react-navigation": "^2.0.24",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "eslint": "^4.19.1",
    "jest": "^23.6.0",
    "jest-fetch-mock": "^1.6.6",
    "metro-react-native-babel-preset": "0.48.3",
    "mocha": "^5.2.0",
    "mock-async-storage": "^2.0.3",
    "react-native-typescript-transformer": "^1.2.10",
    "react-test-renderer": "16.6.0-alpha.8af6728",
    "typescript": "^3.1.3"
  },
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "node node_modules/jest/bin/jest.js",
    "watchTest": "node node_modules/jest/bin/jest.js --watch",
    "check": "npm run tsc && npm run lint && npm run test",
    "tsc": "tsc --skipLibCheck",
    "lint": "tslint src/**/*.ts",
    "build": "npm run clean && npm run tsc --",
    "clean": "rm -rf build"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "\\.(ts|tsx)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|react-navigation)/"
    ],
    "testRegex": "(/test/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/lib/",
      "<rootDir>/build/",
      "<rootDir>/e2e/"
    ],
    "cacheDirectory": ".jest/cache",
    "globals": {
      "ts-jest": {
        "skipBabel": true
      }
    }
  },
  "dependencies": {
    "buffer": "^5.2.1",
    "elliptic": "^6.4.1",
    "immutable": "^3.8.2",
    "js-sha3": "^0.8.0",
    "node-libs-browser": "^2.1.0",
    "react": "16.6.0-alpha.8af6728",
    "react-native": "0.57.7",
    "node-libs-react-native": "^1.0.3",
    "react-native-camera": "^1.3.1",
    "react-native-communications": "^2.2.1",
    "react-native-crypto": "^2.1.2",
    "react-native-easy-markdown": "^1.2.0",
    "react-native-fs": "^2.12.1",
    "react-native-get-real-path": "git+https://github.com/Wraptime/react-native-get-real-path.git",
    "react-native-gravatar": "^1.0.2",
    "react-native-image-picker": "^0.27.1",
    "react-native-onboarding-swiper": "^0.9.0",
    "react-native-image-resizer": "^1.0.0",
    "react-native-parse-html": "^2.2.6",
    "react-native-qrcode-scanner": "^1.1.0",
    "react-native-qrcode-svg": "^5.1.1",
    "react-native-securerandom": "^0.3.0",
    "react-native-settings-list": "^1.8.0",
    "react-native-svg": "^8.0.8",
    "react-native-util": "^1.0.2",
    "react-native-vector-icons": "^6.0.2",
    "react-native-xml2js": "^1.0.3",
    "react-navigation": "^2.18.2",
    "react-redux": "^5.1.0",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-persist-transform-immutable": "^5.0.0",
    "redux-thunk": "^2.3.0",
    "stream-browserify": "^2.0.1",
    "string_decoder": "^1.1.1",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslib": "^1.9.3",
    "url": "^0.11.0",
    "util": "^0.10.4",
    "vm-browserify": "^1.1.0"
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/postmodern.app",
        "build": "xcodebuild -project ios/postmodern.xcodeproj -scheme postmodern -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 7"
      }
    },
    "test-runner": "mocha"
  }
}
